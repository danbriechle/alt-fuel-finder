<h1><%= @stations.count %> Closest Stations </h1>
<% @stations.each do |station| %>
<div class="stations-<%= station.name %>">
  <h2>Name: <%= station.name %></h2>
    <li>Access Times: <%= station.access_times %></li>
    <li>Address: <%= station.address %></li>
    <li>Distance: <%= station.distance %> Miles</li>
    <li>Fuel Type: <%= station.fuel_type %></li>
</div>
  <%end %>
